<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:eu.ydp.empiria.player.client.components">
	<ui:with type="eu.ydp.empiria.player.client.resources.StyleNameConstants" field="res" />
	<g:FlowPanel ui:field="mainPanel" styleName="{res.QP_SLIDESHOW}">
		<g:FlowPanel ui:field="titlePanel" styleName="{res.QP_SLIDESHOW_TITLE_PANEL}">
		</g:FlowPanel>
		<g:FlowPanel ui:field="slidesPanel" styleName="{res.QP_SLIDESHOW_SLIDES_PANEL}">
		</g:FlowPanel>
		<g:FlowPanel ui:field="buttonsPanel" styleName="{res.QP_SLIDESHOW_BUTTON_PANEL}">
			<c:TwoStateButton ui:field="playButton"/>
			<g:PushButton ui:field="stopButton" styleName="{res.QP_SLIDESHOW_BUTTON_STOP}"/>
			<g:PushButton ui:field="previousButton" styleName="{res.QP_SLIDESHOW_BUTTON_PREVIOUS}"/>
			<g:PushButton ui:field="nextButton" styleName="{res.QP_SLIDESHOW_BUTTON_NEXT}"/>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>