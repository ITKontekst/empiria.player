<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with type="eu.ydp.empiria.player.client.module.button.ButtonStyleNameConstants" field="res"/>
    <g:FlowPanel styleName="{res.QP_BUTTON_MODULE}">
        <g:Anchor ui:field="anchor" target="_blank">
            <div>
                <div ui:field="description" class="{res.QP_BUTTON_MODULE_DESCRIPTION}"/>
                <div class="{res.QP_BUTTON_MODULE_BUTTON}"/>
            </div>
        </g:Anchor>
    </g:FlowPanel>
</ui:UiBinder>
