<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:eu.ydp.gwtutil.client.ui.button">
    <ui:with type="eu.ydp.empiria.player.client.resources.StyleNameConstants" field="res"/>
    <g:FlowPanel ui:field="container" styleName="{res.QP_IMG_DEFAULT}">
        <g:Image ui:field="image" styleName="{res.QP_IMG}"/>
        <b:CustomPushButton ui:field="fullScreenButton" styleName="{res.QP_MEDIA_FULLSCREEN_BUTTON}"/>
    </g:FlowPanel>
</ui:UiBinder>