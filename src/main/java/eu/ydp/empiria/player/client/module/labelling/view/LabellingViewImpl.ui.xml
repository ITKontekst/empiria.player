<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with type="eu.ydp.empiria.player.client.module.labelling.LabellingStyleNameConstants" field="styles"/>
    <g:SimplePanel styleName="{styles.QP_LABELLING}">
        <g:AbsolutePanel ui:field="inner" styleName="{styles.QP_LABELLING_INNER}">
            <g:at left='0' top='0'>
                <g:Image ui:field="image" styleName="{styles.QP_LABELLING_BACKGROUND}"/>
            </g:at>
            <g:at left='0' top='0'>
                <g:AbsolutePanel ui:field="container" styleName="{styles.QP_LABELLING_CHILDREN}"/>
            </g:at>
        </g:AbsolutePanel>
    </g:SimplePanel>
</ui:UiBinder> 
