<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:eu.ydp.gwtutil.client.ui.button">
    <ui:with type="eu.ydp.empiria.player.client.module.feedback.FeedbackStyleNameConstants" field="res"/>
    <g:FlowPanel ui:field="feedbackTextModule" styleName="{res.QP_FEEDBACK_TEXT_MODULE}">
        <b:CustomPushButton ui:field="feedbackCloseButton" styleName="{res.QP_FEEDBACK_TEXT_MODULE_CLOSE_BUTTON}"/>
        <b:CustomPushButton ui:field="feedbackShowButton" styleName="{res.QP_FEEDBACK_TEXT_MODULE_SHOW_BUTTON}"/>
        <g:FlowPanel ui:field="feedbackTextPanel" styleName="{res.QP_FEEDBACK_TEXT}"/>
    </g:FlowPanel>
</ui:UiBinder> 