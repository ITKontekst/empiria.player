<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="eu.ydp.empiria.player.client.resources.StyleNameConstants" field="res"></ui:with>
	<g:FlowPanel ui:field="panel" styleName="{res.QP_IDENTIFICATION_MODULE}">
	<g:FlowPanel ui:field="optionPanel" styleName="{res.QP_IDENTIFICATION_OPTION}">
		<g:AbsolutePanel ui:field="container" styleName="{res.QP_IDENTIFICATION_OPTION_CONTAINER}">
			<g:at left='0' top='0'>
				<g:Widget ui:field="contentWidget">				
				</g:Widget>
			</g:at>	
			<g:at left='0' top='0'>
				<g:AbsolutePanel ui:field="cover" styleName="{res.QP_IDENTIFICATION_OPTION_COVER}"/>
			</g:at>	
		</g:AbsolutePanel>
	</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 