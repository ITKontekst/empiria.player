<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with type="eu.ydp.empiria.player.client.resources.StyleNameConstants" field="res"></ui:with>
    <g:FlowPanel ui:field="wrapper" styleName="{res.QP_ACCORDION_SECTION} {res.QP_ACCORDION_HIDDEN}">
        <g:SimplePanel ui:field="title" styleName="{res.QP_ACCORDION_SECTION_TITLE}"></g:SimplePanel>
        <g:SimplePanel ui:field="content" styleName="{res.QP_ACCORDION_SECTION_CONTENT} {res.QP_ZERO_WIDTH} {res.QP_ZERO_HEIGHT}"></g:SimplePanel>
    </g:FlowPanel>
</ui:UiBinder>