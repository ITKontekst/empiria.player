<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:eu.ydp.gwtutil.client.ui.button">
    <ui:with type="eu.ydp.empiria.player.client.module.dictionary.DictionaryStyleNameConstants" field="res"/>
    <g:FlowPanel styleName="{res.QP_DICTIONARY_POPUP}">
        <b:CustomPushButton ui:field="closeButton" styleName="{res.QP_DICTIONARY_POPUP_CLOSE_BUTTON}"/>
        <g:FlowPanel ui:field="container" styleName="{res.QP_DICTIONARY_POPUP_CONTAINER}"/>
    </g:FlowPanel>
</ui:UiBinder> 
