<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with type="eu.ydp.empiria.player.client.view.ViewStyleNameConstants" field="res"></ui:with>
    <g:FlowPanel ui:field="playerPanel" styleName="{res.QP_PLAYER}">
        <g:FlowPanel ui:field="headerPanel" styleName="{res.QP_PLAYER_HEADER}"/>
        <g:FlowPanel ui:field="assessmentPanel" styleName="{res.QP_PLAYER_BODY}"/>
        <g:FlowPanel ui:field="footerPanel" styleName="{res.QP_PLAYER_FOOTER}"/>
    </g:FlowPanel>
</ui:UiBinder>
