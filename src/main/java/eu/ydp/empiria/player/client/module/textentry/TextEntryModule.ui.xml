<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with type="eu.ydp.empiria.player.client.module.textentry.TextEntryStyleNameConstants" field="res"/>
    <g:FlowPanel ui:field="moduleWidget" styleName="{res.QP_TEXT_TEXTENTRY}">
        <g:FlowPanel styleName="{res.QP_TEXT_TEXTENTRY_PREFIX}"/>
