<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with type="eu.ydp.empiria.player.client.module.slideshow.SlideshowStyleNameConstants" field="res"/>
    <g:FlowPanel ui:field="mainPanel" styleName="{res.QP_SLIDESHOW}">
        <g:FlowPanel ui:field="titlePanel" styleName="{res.QP_SLIDESHOW_TITLE_PANEL}">
        </g:FlowPanel>
        <g:FlowPanel ui:field="slidesPanel" styleName="{res.QP_SLIDESHOW_SLIDES_PANEL}">
        </g:FlowPanel>
        <g:FlowPanel ui:field="pagerPanel">
        </g:FlowPanel>
    </g:FlowPanel>
</ui:UiBinder>