<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="eu.ydp.empiria.player.client.resources.StyleNameConstants" field="res"/>
	<g:FlowPanel ui:field="slidePanel" styleName="{res.QP_SLIDESHOW_SLIDE_PANEL}">
		<g:FlowPanel ui:field="imgPanel" styleName="{res.QP_SLIDESHOW_SLIDE_IMAGE_PANEL}">
			<g:Image ui:field="image" styleName="{res.QP_SLIDESHOW_SLIDE_IMAGE}">
			</g:Image>
		</g:FlowPanel>
		<g:FlowPanel ui:field="titlePanel" styleName="{res.QP_SLIDESHOW_SLIDE_TITLE_PANEL}">
			<g:HTMLPanel ui:field="inlineTitleText" styleName="{res.QP_TEXT_INLINE}">
				<g:InlineLabel ui:field="titleText" styleName="{res.QP_TEXT}"/>
			</g:HTMLPanel>
		</g:FlowPanel>
		<g:FlowPanel ui:field="narrationPanel" styleName="{res.QP_SLIDESHOW_SLIDE_NARRATION_PANEL}">
			<g:HTMLPanel ui:field="inlineNarrationText" styleName="{res.QP_TEXT_INLINE}">
				<g:InlineLabel ui:field="narrationText" styleName="{res.QP_TEXT}"/>
			</g:HTMLPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 