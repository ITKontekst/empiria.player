<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:v="urn:import:eu.ydp.empiria.player.client.module.dictionary.external.view">
	<ui:with type="eu.ydp.empiria.player.client.resources.StyleNameConstants" field="res"/>
	<g:FlowPanel styleName="{res.QP_DICTIONARY_MAIN}">
		<g:FlowPanel ui:field="containerView" styleName="{res.QP_DICTIONARY_CONTENT_CONTAINER}">
			<v:MenuView ui:field="menuView" />
			<v:ExplanationView ui:field="explanationView" />
		</g:FlowPanel>
		<g:SimplePanel styleName="{res.QP_DICTIONARY_HEADER_PANEL}">
			<g:PushButton ui:field="exitButton" styleName="{res.QP_DICTIONARY_EXIT_BUTTON}"/>
		</g:SimplePanel>
	</g:FlowPanel>
</ui:UiBinder> 