<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui" >

    <g:FlowPanel styleName="dict-explanation">
    	<g:PushButton styleName="dict-explanation-back-button" ui:field="backButton" />
    	<g:FlowPanel styleName="dict-explanation-post" ui:field="postPanel">
    		<g:Label styleName="dict-explanation-post-label" ui:field="postLabel"/>
    	</g:FlowPanel>
    	<g:FlowPanel styleName="dict-explanation-ang" ui:field="angPanel">
    		<g:Label styleName="dict-explanation-ang-label" ui:field="angLabel"/>
    	</g:FlowPanel>
    	<g:FlowPanel styleName="dict-explanation-pol" ui:field="polPanel">
    		<g:InlineHTML styleName="dict-explanation-pol-label" ui:field="polLabel"/>
    	</g:FlowPanel>
    	<g:FocusPanel styleName="dict-explanation-desc" ui:field="descPanel">
    		<g:Label styleName="dict-explanation-desc-label" ui:field="descLabel"/>
    	</g:FocusPanel>
    	<g:PushButton styleName="dict-explanation-play-button" ui:field="playButton" text="play" />
    </g:FlowPanel>
</ui:UiBinder>