<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui" >
	<ui:with type="eu.ydp.empiria.player.client.resources.StyleNameConstants" field="res"/>
    <g:FlowPanel styleName="{res.QP_DICTIONARY_EXPLANATION}">
    	<g:PushButton styleName="{res.QP_DICTIONARY_EXPLANATION_BACK_BUTTON}" ui:field="backButton" />
    	<g:FlowPanel styleName="{res.QP_DICTIONARY_EXPLANATION_POST}" ui:field="postPanel">
    		<g:Label styleName="{res.QP_DICTIONARY_EXPLANATION_POST_LABEL}" ui:field="postLabel"/>
    	</g:FlowPanel>
    	<g:FlowPanel styleName="{res.QP_DICTIONARY_EXPLANATION_LANGUAGE_ANG}" ui:field="angPanel">
    		<g:Label styleName="{res.QP_DICTIONARY_EXPLANATION_LANGUAGE_ANG_LABEL}" ui:field="angLabel"/>
    	</g:FlowPanel>
    	<g:FlowPanel styleName="{res.QP_DICTIONARY_EXPLANATION_LANGUAGE_POL}" ui:field="polPanel">
    		<g:InlineHTML styleName="{res.QP_DICTIONARY_EXPLANATION_LANGUAGE_POL_LABEL}" ui:field="polLabel"/>
    	</g:FlowPanel>
    	<g:FocusPanel styleName="{res.QP_DICTIONARY_EXPLANATION_DESCRIPTION}" ui:field="descPanel">
    		<g:Label styleName="{res.QP_DICTIONARY_EXPLANATION_DESCRIPTION_LABEL}" ui:field="descLabel"/>
    	</g:FocusPanel>
    	<g:PushButton styleName="{res.QP_DICTIONARY_EXPLANATION_PLAY_BUTTON}" ui:field="playButton" text="play" />
    </g:FlowPanel>
</ui:UiBinder>