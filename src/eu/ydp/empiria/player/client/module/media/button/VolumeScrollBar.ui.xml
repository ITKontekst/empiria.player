<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:eu.ydp.empiria.player.client.module.media.button">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:FlowPanel ui:field="progressBar" styleName="qp-media-volume-scrollbar">
		<g:FlowPanel styleName="qp-media-volume-scrollbar-top" />
		<g:FlowPanel ui:field="mainProgressDiv" styleName="qp-media-volume-scrollbar-center">
			<g:FlowPanel styleName="qp-media-volume-scrollbar-center-info" />
			<g:FlowPanel ui:field="beforeButton" styleName="qp-media-volume-scrollbar-center-button-top" />
			<m:SimpleMediaButton ui:field="button" styleName="qp-media-volume-scrollbar-center-button" />
			<g:FlowPanel ui:field="afterButton" styleName="qp-media-volume-scrollbar-center-button-bottom" />
		</g:FlowPanel>
		<g:FlowPanel styleName="qp-media-volume-scrollbar-bottom" />
	</g:FlowPanel>
</ui:UiBinder>