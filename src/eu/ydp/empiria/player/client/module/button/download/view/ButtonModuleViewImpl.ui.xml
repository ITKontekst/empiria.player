<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"  xmlns:b="urn:import:eu.ydp.gwtutil.client.ui.button" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="eu.ydp.empiria.player.client.resources.StyleNameConstants" field="res" />
	<g:FlowPanel styleName="{res.QP_BUTTON_MODULE}">
		<g:FlowPanel ui:field="description" styleName="{res.QP_BUTTON_MODULE_DESCRIPTION}"/>
		<g:Anchor ui:field="anchor" target="_blank">
			<div class="{res.QP_BUTTON_MODULE_BUTTON}"/>
		</g:Anchor>
	</g:FlowPanel>
</ui:UiBinder>