<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='empiria.player'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.user.ImageBundle' />
	<inherits name="com.google.gwt.http.HTTP" />
	<inherits name="com.google.gwt.xml.XML" />
	<inherits name="com.google.gwt.json.JSON" />
	<inherits name='com.allen_sauer.gwt.voices.gwt-voices' />
	<inherits name='com.mathplayer.Math_player' />
	<inherits name='eu.ydp.gwtutil.YdpGwtUtil' />
	<inherits name='eu.ydp.canvasadapter.canvasadapter' />
	<inherits name='eu.ydp.jsfilerequest.jsfilerequest' />
	<!-- google gin -->
	<inherits name="com.google.gwt.inject.Inject" />

	<set-property name="compiler.emulatedStack" value="true" />
	<set-property name="compiler.stackMode" value="strip" />
	<set-configuration-property name="compiler.emulatedStack.recordFileNames" value="true" />

	<!-- Specify the app entry point class. -->
	<entry-point class='eu.ydp.empiria.player.client.PlayerEntryPoint' />

	<!-- css parser -->

	<script src="video/AC_RunActiveContent.js" />
	<script src="video/FAVideo.js" />
	<script src="jscss/cssparser.js" />

	<!-- VIDEO -->
	<replace-with class="eu.ydp.empiria.player.client.module.object.impl.FlashVideoImpl">
		<when-type-is class="eu.ydp.empiria.player.client.module.object.impl.Video" />
	</replace-with>

	<replace-with class="eu.ydp.empiria.player.client.module.object.impl.HTML5VideoImpl">
		<when-type-is class="eu.ydp.empiria.player.client.module.object.impl.OggVideo" />
	</replace-with>

	<!-- Use video tag for iphone browser -->
	<replace-with class="eu.ydp.empiria.player.client.module.object.impl.HTML5VideoImpl">
		<when-type-is class="eu.ydp.empiria.player.client.module.object.impl.Video" />
		<any>
			<when-property-is name="user.agent" value="safari" />
		</any>
	</replace-with>

	<replace-with class="eu.ydp.empiria.player.client.module.object.impl.HTML5VideoAndroidImpl">
		<when-type-is class="eu.ydp.empiria.player.client.module.object.impl.Video" />
		<any>
			<when-property-is name="mobile.user.agent" value="android23" />
			<when-property-is name="mobile.user.agent" value="android321" />
			<when-property-is name="mobile.user.agent" value="android4" />
		</any>
	</replace-with>
	<replace-with class="eu.ydp.empiria.player.client.module.object.impl.HTML5VideoAndroidImpl">
		<when-type-is class="eu.ydp.empiria.player.client.module.object.impl.OggVideo" />
		<any>
			<when-property-is name="mobile.user.agent" value="android23" />
			<when-property-is name="mobile.user.agent" value="android321" />
			<when-property-is name="mobile.user.agent" value="android4" />
		</any>
	</replace-with>
	<replace-with class="eu.ydp.empiria.player.client.module.object.impl.HTML5VideoAndroid3Impl">
		<when-type-is class="eu.ydp.empiria.player.client.module.object.impl.Video" />
		<when-property-is name="mobile.user.agent" value="android3" />
	</replace-with>
	<replace-with class="eu.ydp.empiria.player.client.module.object.impl.HTML5VideoAndroid3Impl">
		<when-type-is class="eu.ydp.empiria.player.client.module.object.impl.OggVideo" />
		<when-property-is name="mobile.user.agent" value="android3" />
	</replace-with>
	<!-- END VIDEO -->

	<!-- Progressbar dla androida w media -->
	<replace-with class="eu.ydp.empiria.player.client.module.media.button.MediaProgressBarImpl">
		<when-type-is class="eu.ydp.empiria.player.client.module.media.button.MediaProgressBar" />
	</replace-with>
	<replace-with class="eu.ydp.empiria.player.client.module.media.button.MediaProgressBarAndroid">
		<when-type-is class="eu.ydp.empiria.player.client.module.media.button.MediaProgressBar" />
		<any>
			<when-property-is name="mobile.user.agent" value="android23" />
			<when-property-is name="mobile.user.agent" value="android321" />
			<when-property-is name="mobile.user.agent" value="android4" />
			<when-property-is name="mobile.user.agent" value="android3" />
		</any>
	</replace-with>
	<!-- end progressbar -->

	<!-- AUDIO -->
	<!-- link as default value for audio -->
	<replace-with class="eu.ydp.empiria.player.client.module.object.impl.EmbedAudioImpl">
		<when-type-is class="eu.ydp.empiria.player.client.module.object.impl.Audio" />
	</replace-with>

	<replace-with class="eu.ydp.empiria.player.client.module.object.impl.HTML5AudioImpl">
		<when-type-is class="eu.ydp.empiria.player.client.module.object.impl.OggAudio" />
	</replace-with>

	<!-- Use audio tag for iphone browser -->
	<replace-with class="eu.ydp.empiria.player.client.module.object.impl.HTML5AudioImpl">
		<when-type-is class="eu.ydp.empiria.player.client.module.object.impl.Audio" />
		<any>
			<when-property-is name="user.agent" value="safari" />
		</any>
	</replace-with>
	<!-- END AUDIO -->

	<!-- default implementation of sound executor -->
	<replace-with class="eu.ydp.empiria.player.client.controller.extensions.internal.sound.SoundExecutorSwf">
		<when-type-is class="eu.ydp.empiria.player.client.controller.extensions.internal.sound.SoundExecutor" />
	</replace-with>

	<!-- html5 implementation of sound executor -->
	<replace-with class="eu.ydp.empiria.player.client.controller.extensions.internal.sound.SoundExecutorHtml5">
		<when-type-is class="eu.ydp.empiria.player.client.controller.extensions.internal.sound.SoundExecutor" />
		<any>
			<when-property-is name="user.agent" value="safari" />
		</any>
	</replace-with>
	
	<replace-with class="eu.ydp.empiria.player.client.module.img.ExplorableImgWindowCanvas">
		<when-type-is class="eu.ydp.empiria.player.client.module.img.ExplorableImgWindow" />
	</replace-with>
	
	<replace-with class="eu.ydp.empiria.player.client.module.img.ExplorableImgWindowImg">
		<when-type-is class="eu.ydp.empiria.player.client.module.img.ExplorableImgWindow" />
		<any>
			<when-property-is name="user.agent" value="ie6" />
			<when-property-is name="user.agent" value="ie8" />
		</any>
	</replace-with>

	<define-linker name="xsiframescripts" class="eu.ydp.empiria.player.core.linker.CrossSizeIframeWithScriptsLinker" />
	<add-linker name="xsiframescripts" />

</module>
